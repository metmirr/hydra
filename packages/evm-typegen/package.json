{
  "name": "evm-typegen",
  "description": "Hydra-cli plugin for generating types for substrate evm pallet",
  "version": "0.0.1",
  "author": "metmirr @metmirr",
  "bin": {
    "evm-typegen": "./bin/run"
  },
  "bugs": "https://github.com/joystream/hydra/issues",
  "scripts": {
    "build": "rm -rf lib && tsc --build tsconfig.json",
    "postpack": "rm -f oclif.manifest.json",
    "posttest": "eslint . --ext .ts --config .eslintrc",
    "prepack": "rm -rf lib && tsc -b && oclif-dev manifest && oclif-dev readme",
    "run-dev": "rm -rf generated/types && node ./bin/run",
    "test": "nyc --extension .ts mocha --forbid-only \"test/**/*.test.ts\"",
    "version": "oclif-dev readme && git add README.md"
  },
  "dependencies": {
    "@oclif/command": "^1",
    "@oclif/config": "^1",
    "@oclif/plugin-help": "^2",
    "tslib": "^1",
    "@typechain/truffle-v5": "^4.0.0",
    "@types/glob": "^7.1.3",
    "abi-decoder": "^2.3.0",
    "bn.js": "^5.1.3",
    "glob": "^7.1.6",
    "typechain": "^4.0.1"
  },
  "devDependencies": {
    "@types/bn.js": "^4.11.6",
    "@oclif/dev-cli": "^1",
    "@oclif/test": "^1",
    "@types/chai": "^4",
    "@types/mocha": "^5",
    "@types/node": "^10",
    "chai": "^4",
    "eslint": "^5.13",
    "eslint-config-oclif": "^3.1",
    "eslint-config-oclif-typescript": "^0.1",
    "globby": "^10",
    "mocha": "^5",
    "nyc": "^14",
    "ts-node": "^8",
    "typescript": "^3.3"
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "files": [
    "/bin",
    "/lib"
  ],
  "homepage": "https://github.com/joystream/hydra",
  "keywords": [
    "oclif"
  ],
  "license": "MIT",
  "main": "lib/index.js",
  "oclif": {
    "commands": "./lib/commands",
    "bin": "evm-typegen",
    "plugins": [
      "@oclif/plugin-help"
    ]
  },
  "repository": "https://github.com/joystream/hydra",
  "types": "lib/index.d.ts"
}
